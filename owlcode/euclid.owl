program 'euclids';
begin
    let a: int := 72;
    let b: int := 42;
    func gcd(a: int, b: int) begin
        while (a != b) begin
            if (b < a) then
                a := a - b;
            else
                b := b - a;
            end;
        end;
        return a;
    end;
    print "Enter first number: ";
    input a;
    print "Enter second number: ";
    input b;
    print (gcd(a, b) + "\n");
end