program 'fibonacciEx';
begin
    let prev: int := 1;
    let next: int := 2;
    let curr: int := 0;
    let x: int := 1;
    func calcAndPrint() begin
        next := (prev + curr);
        print (next + " ");
        prev := curr;
        curr := next;
    end

    while (x <= 10) begin
        calcAndPrint();
        x := (x + 1);
    end;
    
    print "\n";
end