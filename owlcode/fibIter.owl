program 'fibonacciEx';
begin
    let prev := 1;
    let next := 2;
    let curr := 0;
    let x := 1;
    func calcAndPrint() begin
        next := (prev + curr);
        print (next + " ");
        prev := curr;
        curr := next;
    end

    while (x <= 10) begin
        calcAndPrint();
        x := (x + 1);
    end;
    
    print "\n";
end