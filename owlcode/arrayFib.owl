program 'ArrayFib';
begin
    let m: int := 3;
    let n: int := 5;
    let fib[52]: int;
    fib[1] := 0;
    fib[2] := 1;
    func nthFib(n: int) begin
        let i: int := 3;
        while (i < n) begin
            fib[i] := fib[i - 1] + fib[i - 2];
            i := i + 1;
        end;
        return fib[i - 1];
    end
    print "Enter the number of fibonacci numbers to display: ";
    input n;
    m := 3;
    while (m <= n+2) begin
        print (nthFib(m) + " ");
        m := m + 1;
    end;
    print "\n";
end